{
  "functions": {
    "apps/web/src/app/api/**/*.ts": {
      "runtime": "@vercel/node@3.0.11"
    }
  },
  "buildCommand": "cd apps/web && pnpm build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "pnpm install --frozen-lockfile",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/apps/web/:path*"
    }
  ]
}